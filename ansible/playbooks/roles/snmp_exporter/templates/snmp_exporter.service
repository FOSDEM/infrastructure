[Unit]
Description=Prometheus SNMP Exporter

[Service]
User=nobody
Group=nogroup
EnvironmentFile=-/etc/default/snmp_exporter
ExecStart={{ snmp_exporter_path }}/snmp_exporter -config.file {{ snmp_exporter_install_path }}/snmp.yml -log.level debug
KillMode=process
Restart=always

[Install]
WantedBy=multi-user.target
